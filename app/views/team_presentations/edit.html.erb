<%= form_for(@team_presentation, url: team_presentation_path(@team_presentation), html: { method: :put }) do |form| %>
  <div class="form-group">
  <%= form.label "The name of your site"%>
  <%= form.text_field :title, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label "Used in your theme, metadata and search results"%>
    <%= form.text_field :description, class: "form-control" %>
  </div>
  <div class="actions">
      <%= form.submit "Save settings", class: "btn btn-primary k-story-btn"%>
  </div>
<%end%>  